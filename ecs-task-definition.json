{
    "family": "elb-autoscalle-task-definition",
    "executionRoleArn": "arn:aws:iam::092663235681:role/ecs-devops-sandbox-execution-role",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "alb-scale-cont",
            "image": "alb-http-ecs:00000",
            "portMappings": [
                {
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "50",
    "memory": "128"
}
